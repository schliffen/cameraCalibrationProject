<opencv_storage>
<Settings>
  <!-- path to the video file and video setting-->
  <video_setting_path>"../sampleVideo/az_01/ba0d1cca-cc92-4fde-8ec4-0732a76b2760_j.json"</video_setting_path>
  <video_file_path>"../sampleVideo/az_01/ba0d1cca-cc92-4fde-8ec4-0732a76b2760_v.mp4"</video_file_path>
  <radar_trigger_frame_path>"../sampleVideo/az_01/ba0d1cca-cc92-4fde-8ec4-0732a76b2760_s.jpg"</radar_trigger_frame_path>
  <!--  path to the test videos!  -->
  <test_video_setting_path>"../sampleVideo/az_01/ba0d1cca-cc92-4fde-8ec4-0732a76b2760_j.json"</test_video_setting_path>
  <test_video_file_path>"../sampleVideo/az_01/ba0d1cca-cc92-4fde-8ec4-0732a76b2760_v.mp4"</test_video_file_path>



  <net_cfg_path>"../configs/detector.cfg"</net_cfg_path>
  <net_weights_path>"../configs/detector.weights"</net_weights_path>
  <net_class_names_path>"../configs/labels.names"</net_class_names_path>
  <train_target_position_path>"../configs/train_target_Vehicle_position.txt"</train_target_position_path>
  <test_target_position_path>"../configs/test_target_Vehicle_position.txt"</test_target_position_path>
  <!-- Projection matrix will save here -->
  <estimated_projection_path>"../configs/out_projection_mat.txt"</estimated_projection_path>

  <!-- How many frames to use, for calibration. -->
  <Calibrate_NrOfFrameToUse>25</Calibrate_NrOfFrameToUse>
  <!-- Consider only fy as a free parameter, the ratio fx/fy stays the same as in the input cameraMatrix. 
	   Use or not setting. 0 - False Non-Zero - True-->
  <Calibrate_FixAspectRatio> 1 </Calibrate_FixAspectRatio>
  <!-- If true (non-zero) tangential distortion coefficients  are set to zeros and stay zero.-->
  <Calibrate_AssumeZeroTangentialDistortion>1</Calibrate_AssumeZeroTangentialDistortion>
  <!-- If true (non-zero) the principal point is not changed during the global optimization.-->
  <Calibrate_FixPrincipalPointAtTheCenter> 1 </Calibrate_FixPrincipalPointAtTheCenter>

</Settings>
</opencv_storage>
